{
  "name": "lobbywatch-rooster",
  "version": "0.1.0",
  "engines": {
    "node": "6.9.x"
  },
  "cacheDirectories": [
    "nextjs/node_modules"
  ],
  "description": "Lobbywatch 2017",
  "scripts": {
    "dev": "nodemon -w server.js -w graphql server.js",
    "build": "next build",
    "postinstall": "./bin/build.sh",
    "start": "NODE_ENV=production node server.js",
    "test": "standard 'src/**/*.js' 'pages/**/*.js'",
    "standardize": "standard --fix 'src/**/*.js' 'pages/**/*.js'",
    "klog": "find . -name '*.js' -not -path \"./node_modules*\" -not -path \"./.next*\" | xargs wc -l"
  },
  "author": "Thomas Preusse <t@preus.se> (https://t.preus.se/)",
  "license": "BSD-3-Clause",
  "dependencies": {
    "accept-language": "^3.0.15",
    "apollo-client": "^0.6.0",
    "body-parser": "^1.15.2",
    "cors": "^2.8.1",
    "d3-array": "^1.0.2",
    "d3-collection": "^1.0.2",
    "d3-format": "^1.0.2",
    "d3-hierarchy": "^1.1.1",
    "d3-time": "^1.0.4",
    "d3-time-format": "^2.0.3",
    "dataloader": "^1.3.0",
    "express": "^4.14.0",
    "glamor": "^2.20.22",
    "graphql": "^0.8.2",
    "graphql-server-express": "^0.5.0",
    "graphql-tag": "^1.2.3",
    "graphql-tools": "^0.9.0",
    "gsheets": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
    "lru-cache": "^4.0.2",
    "next": "^2.0.0-beta.31",
    "next-routes": "^1.0.12",
    "querystring": "^0.2.0",
    "react": "^15.4.2",
    "react-apollo": "^0.8.1",
    "react-dom": "^15.4.2",
    "redux": "^3.6.0",
    "striptags": "^3.0.1"
  },
  "devDependencies": {
    "babel-polyfill": "^6.22.0",
    "nodemon": "^1.11.0",
    "standard": "^8.6.0"
  }
}
